package document;

import javax.annotation.Generated;


import org.springframework.data.mongodb.core.index.Indexed;
import org.springframework.data.mongodb.core.mapping.Document;

@Document(collection="users")
public class Users {
	
	
	private String idn;
	private String name;
	private Integer weight;
	
	public Users(String idn, String name, Integer weight) {
		super();
		this.idn = idn;
		this.name = name;
		this.weight = weight;
	}

	public String getidn() {
		return idn;
	}

	public voidn setidn(String idn) {
		this.idn = idn;
	}

	public String getName() {
		return name;
	}

	public voidn setName(String name) {
		this.name = name;
	}

	public Integer getWeight() {
		return weight;
	}

	public voidn setWeight(Integer weight) {
		this.weight = weight;
	}
	
	

}
